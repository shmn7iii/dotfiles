format = """
[┌── ](bold white)$directory$git_branch$git_status$git_commit$ruby$golang$java
[└─](bold white)$character"""

scan_timeout = 10

[character]
success_symbol = "[▶](bold white) "
error_symbol =   "[▶](bold white) "

[shell]
disabled = false

[directory]
style = "bold blue"
read_only_style = "white"
fish_style_pwd_dir_length = 3
format = "[$read_only]($read_only_style)[  ]($style)[$path]($style) "
truncation_length = 4 
truncation_symbol = ".../"

[git_status]
conflicted = " "
untracked  = "？"
stashed    = " "
modified   = " "
staged     = " "
renamed    = " "
deleted    = " "
style = "bold yellow"
ahead = "▲ ${count}"
behind = "▼ ${count}"
diverged = "▲ ${ahead_count} ▼ ${behind_count}"

[git_branch]
format = "on [$symbol$branch](bold purple) "


